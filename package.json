{
  "name": "react-adaptive-text",
  "version": "1.0.5",
  "private": false,
  "scripts": {
    "start": "npx babel src/AdaptiveText.js -w -o dist/AdaptiveText.js -s",
    "build": "npx babel src/AdaptiveText.js -o dist/AdaptiveText.js --minify --presets @babel/preset-react",
    "rollup": "./node_modules/.bin/rollup -c",
    "rollup:test": "rollup src/AdaptiveText.js --file dist/AdaptiveText.js --format iife",
    "publish": "npm publish --access public"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "main": "dist/AdaptiveText.js",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@rollup/plugin-babel": "^5.0.2",
    "@types/react": "^16.8.0",
    "babel-preset-minify": "^0.5.1",
    "rollup": "^2.10.9"
  }
}
